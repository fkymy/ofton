<h1>Posts#new</h1>
<p>Find me in app/views/posts/new.html.erb</p>
<%= form_form :post, url: posts_path do |f| %>
  <div>
    <%= f.label :author %><br>
    <%= f.text_field :author %>
  </div>
  <div>
    <%= f.label :body %><br>
    <%= f.text_area :body %>
  </div>
  <div>
    <%= f.submit %>
  </div>
<% end %>
